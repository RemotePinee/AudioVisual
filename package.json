{
  "name": "audiovisual",
  "version": "1.0.4",
  "description": "The key to unlock all streams.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "author": "The Void",
  "license": "UNLICENSED",
  "devDependencies": {
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-reloader": "^1.2.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "electron-updater": "^6.6.2"
  },
  "build": {
    "appId": "com.void.player",
    "publish": {
      "provider": "github",
      "owner": "RemotePinee",
      "repo": "AudioVisual"
    },
    "productName": "AudioVisual",
    "files": [
      "main.js",
      "index.html",
      "assets/**/*"
    ],
    "win": {
      "target": {
        "target": "nsis",
        "arch": ["x64"]
      },
      "icon": "assets/images/icon.png"
    },
    "compression": "maximum",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "differentialPackage": true
    },
    "electronDownload": {
      "mirror": "https://npm.taobao.org/mirrors/electron/"
    }
  }
}
